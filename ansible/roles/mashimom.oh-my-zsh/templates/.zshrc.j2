# File generated by Ansible, CHANGES HERE MAY NOT PERSIST!!!

export ZSH=$HOME/.oh-my-zsh
export TERM="xterm-256color"

## Theme
ZSH_THEME="{{ ohmyzsh_theme }}"
POWERLEVEL9K_CONTEXT_DEFAULT_FOREGROUND='255'
POWERLEVEL9K_CONTEXT_DEFAULT_BACKGROUND='166'

## plugins
plugins=({{ ohmyzsh_plugins|join(' ') }})

export PATH=$HOME/bin:/usr/local/bin:$PATH

source $ZSH/oh-my-zsh.sh



## aliases
alias prn="print -l $@"
alias fnd="realpath $@"
alias findjar="for f (**/*.jar(.)) jar -tf \"$f\" | grep --label \"$f\" -l `sed 's|\.|/|g' <<< \"$@\"`"


## Extra from bash configuration
if [ -f ~/.bash_aliases ]; then
    . ~/.bash_aliases
fi

if [ -f ~/.fzf.zsh ]; then
    . ~/.fzf.zsh
fi

if [ -f ~/.bash_profiles ]; then
    . ~/.bash_profiles
fi

if [ -f ~/.my_docker.bash ]; then
    . ~/.my_docker.bash
fi
